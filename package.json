{
  "name": "alexa-ability-stash",
  "version": "0.1.0",
  "description": "An alexa-ability middleware for persistent user storage.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint src",
    "pretest": "npm run lint",
    "test": "mocha test",
    "prepublish": "in-publish && npm run test && npm run build || not-in-publish",
    "publish:major": "npm version major && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "postpublish": "git push origin master --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickclaw/alexa-ability-stash"
  },
  "bugs": {
    "url": "http://github.com/nickclaw/alexa-ability-stash/issues"
  },
  "keywords": [
    "store",
    "session",
    "redis",
    "user",
    "amazon",
    "alexa",
    "stash",
    "skill",
    "kit",
    "echo",
    "ability",
    "alexa-ability"
  ],
  "author": {
    "name": "Nicholas Clawson",
    "email": "nickclaw@gmail.com",
    "url": "nickclaw.com"
  },
  "license": "MIT",
  "dependencies": {
    "crc": "^3.4.0",
    "debug": "^2.2.0",
    "express-session": "^1.13.0",
    "lodash": "^4.3.0"
  },
  "devDependencies": {
    "alexa-ability": "^0.11.0",
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
    "in-publish": "^2.0.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
